<%- include("partials/header") -%>

<h1>Compose</h1>

<form action="/compose" method="post">
    <div class="form-group">
        <label>Title</label>
        <input type="text" class="form-control" name="title">
    </div>
    <div class="form-group">
        <label>Post</label>
        <textarea class="form-control" rows="10" name="post"></textarea>
    </div>
    <div class="form-group">
        <button type="submit" class="button-13">Publish</button>

    </div>

</form>

<%- include("partials/footer") -%>